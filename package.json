{
    "name": "lodash",
    "version": "4.17.11",
    "license": "MIT",
    "private": true,
    "main": "lodash.js",
    "engines": {
        "node": ">=4.0.0"
    },
    "scripts": {
        "build": "npm run build:main && npm run build:fp",
        "build:fp": "node lib/fp/build-dist.js",
        "build:fp-modules": "node lib/fp/build-modules.js",
        "build:main": "node lib/main/build-dist.js",
        "build:main-modules": "node lib/main/build-modules.js",
        "doc": "node lib/main/build-doc github && npm run test:doc",
        "doc:fp": "node lib/fp/build-doc",
        "doc:site": "node lib/main/build-doc site",
        "doc:sitehtml": "optional-dev-dependency marky-markdown@^9.0.1 && npm run doc:site && node lib/main/build-site",
        "pretest": "npm run build",
        "style": "npm run style:main && npm run style:fp && npm run style:perf && npm run style:test",
        "style:fp": "jscs fp/*.js lib/**/*.js",
        "style:main": "jscs lodash.js",
        "style:perf": "jscs perf/*.js perf/**/*.js",
        "style:test": "jscs test/*.js test/**/*.js",
        "test": "npm run test:main && npm run test:fp",
        "test:doc": "markdown-doctest doc/*.md",
        "test:fp": "node test/test-fp",
        "test:main": "node test/test",
        "validate": "npm run style && npm run test",
        "compile-testtestfpjs": "babel test\\test-fp.js -o test\\test-fp.js --require babel-polyfill --presets env",
        "compile-fpplaceholderjs": "babel fp\\placeholder.js -o fp\\placeholder.js --require babel-polyfill --presets es2015-without-strict",
        "compile-testsaucelabsjs": "babel test\\saucelabs.js -o test\\saucelabs.js --require babel-polyfill --presets env",
        "compile-fp_baseConvertjs": "babel fp\\_baseConvert.js -o fp\\_baseConvert.js --require babel-polyfill --presets es2015-without-strict",
        "compile-libcommonfilejs": "babel lib\\common\\file.js -o lib\\common\\file.js --require babel-polyfill --presets env",
        "compile-libcommonutiljs": "babel lib\\common\\util.js -o lib\\common\\util.js --require babel-polyfill --presets env",
        "compile-libcommonminifyjs": "babel lib\\common\\minify.js -o lib\\common\\minify.js --require babel-polyfill --presets env",
        "compile-libfpbuilddistjs": "babel lib\\fp\\build-dist.js -o lib\\fp\\build-dist.js --require babel-polyfill --presets env",
        "compile-testassetworkerjs": "babel test\\asset\\worker.js -o test\\asset\\worker.js --require babel-polyfill --presets env",
        "compile-fp_convertBrowserjs": "babel fp\\_convertBrowser.js -o fp\\_convertBrowser.js --require babel-polyfill --presets es2015-without-strict",
        "compile-libcommonmappingjs": "babel lib\\common\\mapping.js -o lib\\common\\mapping.js --require babel-polyfill --presets env",
        "compile-testassettestuijs": "babel test\\asset\\test-ui.js -o test\\asset\\test-ui.js --require babel-polyfill --presets env",
        "compile-libmainbuilddistjs": "babel lib\\main\\build-dist.js -o lib\\main\\build-dist.js --require babel-polyfill --presets env",
        "compile-libmainbuildsitejs": "babel lib\\main\\build-site.js -o lib\\main\\build-site.js --require babel-polyfill --presets env",
        "compile-libfpbuildmodulesjs": "babel lib\\fp\\build-modules.js -o lib\\fp\\build-modules.js --require babel-polyfill --presets env",
        "compile-libmainbuildmodulesjs": "babel lib\\main\\build-modules.js -o lib\\main\\build-modules.js --require babel-polyfill --presets env",
        "compile-libcommonuglifyoptionsjs": "babel lib\\common\\uglify.options.js -o lib\\common\\uglify.options.js --require babel-polyfill --presets env",
        "compile-fp_mappingjs": "babel fp\\_mapping.js -o fp\\_mapping.js --require babel-polyfill --presets es2015-without-strict",
        "compile-testremovejs": "babel test\\remove.js -o test\\remove.js --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-testtestfpjs && npm run compile-fpplaceholderjs && npm run compile-testsaucelabsjs && npm run compile-fp_baseConvertjs && npm run compile-libcommonfilejs && npm run compile-libcommonutiljs && npm run compile-libcommonminifyjs && npm run compile-libfpbuilddistjs && npm run compile-testassetworkerjs && npm run compile-fp_convertBrowserjs && npm run compile-libcommonmappingjs && npm run compile-testassettestuijs && npm run compile-libmainbuilddistjs && npm run compile-libmainbuildsitejs && npm run compile-libfpbuildmodulesjs && npm run compile-libmainbuildmodulesjs && npm run compile-libcommonuglifyoptionsjs && npm run compile-fp_mappingjs && npm run compile-testremovejs",
        "compile": "npm run compile-1_script"
    },
    "devDependencies": {
        "async": "^2.1.4",
        "benchmark": "^2.1.3",
        "chalk": "^1.1.3",
        "cheerio": "^0.22.0",
        "codecov.io": "~0.1.6",
        "coveralls": "^2.11.15",
        "curl-amd": "~0.8.12",
        "docdown": "~0.7.2",
        "dojo": "^1.12.1",
        "ecstatic": "^2.1.0",
        "fs-extra": "~1.0.0",
        "glob": "^7.1.1",
        "istanbul": "0.4.5",
        "jquery": "^3.1.1",
        "jscs": "^3.0.7",
        "lodash": "4.17.3",
        "lodash-doc-globals": "^0.1.1",
        "markdown-doctest": "^0.9.1",
        "optional-dev-dependency": "^2.0.0",
        "platform": "^1.3.3",
        "qunit-extras": "^3.0.0",
        "qunitjs": "^2.1.0",
        "request": "^2.79.0",
        "requirejs": "^2.3.2",
        "sauce-tunnel": "^2.5.0",
        "uglify-js": "2.7.5",
        "webpack": "^1.14.0",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-preset-env": "latest",
        "babel-preset-es2015-without-strict": "latest"
    },
    "greenkeeper": {
        "ignore": [
            "lodash"
        ]
    }
}