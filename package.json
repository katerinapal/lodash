{
    "name": "lodash",
    "version": "4.17.11",
    "license": "MIT",
    "private": true,
    "main": "lodash.js",
    "engines": {
        "node": ">=4.0.0"
    },
    "scripts": {
        "build": "npm run build:main && npm run build:fp",
        "build:fp": "node lib/fp/build-dist.js",
        "build:fp-modules": "node lib/fp/build-modules.js",
        "build:main": "node lib/main/build-dist.js",
        "build:main-modules": "node lib/main/build-modules.js",
        "doc": "node lib/main/build-doc github && npm run test:doc",
        "doc:fp": "node lib/fp/build-doc",
        "doc:site": "node lib/main/build-doc site",
        "doc:sitehtml": "optional-dev-dependency marky-markdown@^9.0.1 && npm run doc:site && node lib/main/build-site",
        "pretest": "npm run build",
        "style": "npm run style:main && npm run style:fp && npm run style:perf && npm run style:test",
        "style:fp": "jscs fp/*.js lib/**/*.js",
        "style:main": "jscs lodash.js",
        "style:perf": "jscs perf/*.js perf/**/*.js",
        "style:test": "jscs test/*.js test/**/*.js",
        "test": "npm run test:main && npm run test:fp",
        "test:doc": "markdown-doctest doc/*.md",
        "test:fp": "node test/test-fp",
        "test:main": "node test/test",
        "validate": "npm run style && npm run test",
        "compile-test": "babel test --out-dir test --require babel-polyfill --presets env",
        "compile-fp": "babel fp --out-dir fp --require babel-polyfill --presets env",
        "compile-libcommon": "babel lib\\common --out-dir lib\\common --require babel-polyfill --presets env",
        "compile-libfp": "babel lib\\fp --out-dir lib\\fp --require babel-polyfill --presets env",
        "compile-testasset": "babel test\\asset --out-dir test\\asset --require babel-polyfill --presets env",
        "compile-libmain": "babel lib\\main --out-dir lib\\main --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-test && npm run compile-fp && npm run compile-libcommon && npm run compile-libfp && npm run compile-testasset && npm run compile-libmain",
        "compile": "npm run compile-1_script"
    },
    "devDependencies": {
        "async": "^2.1.4",
        "benchmark": "^2.1.3",
        "chalk": "^1.1.3",
        "cheerio": "^0.22.0",
        "codecov.io": "~0.1.6",
        "coveralls": "^2.11.15",
        "curl-amd": "~0.8.12",
        "docdown": "~0.7.2",
        "dojo": "^1.12.1",
        "ecstatic": "^2.1.0",
        "fs-extra": "~1.0.0",
        "glob": "^7.1.1",
        "istanbul": "0.4.5",
        "jquery": "^3.1.1",
        "jscs": "^3.0.7",
        "lodash": "4.17.3",
        "lodash-doc-globals": "^0.1.1",
        "markdown-doctest": "^0.9.1",
        "optional-dev-dependency": "^2.0.0",
        "platform": "^1.3.3",
        "qunit-extras": "^3.0.0",
        "qunitjs": "^2.1.0",
        "request": "^2.79.0",
        "requirejs": "^2.3.2",
        "sauce-tunnel": "^2.5.0",
        "stacktrace-js": "^2.0.2",
        "uglify-js": "2.7.5",
        "webpack": "^1.14.0",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-plugin-transform-object-rest-spread": "latest",
        "babel-preset-env": "latest"
    },
    "greenkeeper": {
        "ignore": [
            "lodash"
        ]
    }
}